{
  "name": "backup-gerente/laravel",
  "description": "Database backup manager seamlessly integrated with Laravel 5, 6 or 8 with user-definable procedures and support for S3, Dropbox, FTP, SFTP, and more.",
  "authors": [
    {
      "name": "Shawn McCool || Renato Lucena",
      "email": "cpdrenato@gmail.com",
      "homepage": "https://renatolucena.net"
    }
  ],
  "require": {
    "backup-manager/backup-manager": "^3.0",
    "php": "^7.3||^7.4||^8.0",
    "symfony/process": "^3||^4||^5",
    "illuminate/support": "^5.5||^6||^7||^8||^9",
    "illuminate/container": "^5.5||^6||^7||^8||^9",
    "illuminate/console": "^5.5||^6||^7||^8||^9"
  },
  "require-dev": {
    "mockery/mockery": "dev-master",
    "satooshi/php-coveralls": "~0.6"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "BackupManager\\Laravel\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    },
    "laravel": {
      "providers": [
        "BackupManager\\Laravel\\Laravel55ServiceProvider"
      ]
    }
  },
  "license": "MIT"
}
